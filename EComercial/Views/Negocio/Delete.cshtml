@model EComercial.Models.Negocio

@{
    ViewBag.Title = "Eliminar";
}


<div class="panel panel-primary">
    <div class="panel-heading">
        <h2 class="panel-title">Negocios</h2>
    </div>
    <div class="panel-body">
        <div class="col-md-3">
            <img src="~/Images/e.png" width="80" height="80" />
            <div>    Nombre: @Html.DisplayFor(model => model.Nombre) </div>
            <div>    Dirección: @Html.DisplayFor(model => model.Direccion)</div>
            <div>    CUIT: @Html.DisplayFor(model => model.Cuit)</div>
            <br />
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div>
                    <input type="submit" class="btn-primary" value="Eliminar" /> </div>
    <div>
        @Html.ActionLink("Volver al listado", "Index")
    </div>
    }
</div>
    </div>
</div>


